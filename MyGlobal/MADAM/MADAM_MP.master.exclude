    Function SetFocusState(ByVal ImgNum As String) As Integer
        Dim img As HtmlImage = CType(Me.FindControl("Image" + ImgNum), HtmlImage)
        If LCase(Request.ServerVariables("PATH_INFO")) Like "*main_1*" Then
            img.Attributes.Add("src", "./" + ImgFolder + "/menu_up_15.jpg")
        Else
            img.Attributes.Add("src", "./" + ImgFolder + "/menu_up_" + ImgNum + ".jpg")
        End If   
        img.Attributes.Add("onMouseOver", "return false;")
        img.Attributes.Add("onMouseOut", "return false;")
        Return 1
    End Function
    Protected Sub lan_Click(ByVal sender As Object, ByVal e As System.EventArgs)
        Dim linkbt As LinkButton = CType(sender, LinkButton)
        Select Case linkbt.ID.Trim.ToLower
