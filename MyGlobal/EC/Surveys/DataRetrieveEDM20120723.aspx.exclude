        Me.CheckBoxAppDomain2.Checked = False
        Me.CheckBoxAppDomain3.Checked = False
        Me.CheckBoxAppDomain4.Checked = False
        Me.CheckBoxAppDomain5.Checked = False

        Me.CheckBoxCurrUseProd1.Checked = True
        Me.CheckBoxCurrUseProd2.Checked = False
        Me.CheckBoxCurrUseProd3.Checked = False
        Me.CheckBoxCurrUseProd4.Checked = False
        Me.CheckBoxCurrUseProd5.Checked = False

        Me.RadioButtonBorrow1.SelectedIndex = 0
        
    End Sub

    
