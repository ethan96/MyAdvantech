                Session("Browser_lan") = "zh-cn"
            Case "lan2"
                Session("Browser_lan") = "zh-tw"
            Case Else               
                Session("Browser_lan") = "en"
        End Select       
        If Request.ServerVariables("HTTP_REFERER") IsNot Nothing Then Response.Redirect(Request.ServerVariables("HTTP_REFERER"))
    End Sub
 
