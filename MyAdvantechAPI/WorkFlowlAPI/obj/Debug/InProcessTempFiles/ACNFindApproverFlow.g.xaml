                                          <FlowDecision.True>
                                            <FlowStep x:Name="__ReferenceID9">
                                              <Assign>
                                                <Assign.To>
                                                  <OutArgument x:TypeArguments="x:String">
                                                    <mca:CSharpReference x:TypeArguments="x:String">LineItem.GPStatus</mca:CSharpReference>
                                                  </OutArgument>
                                                </Assign.To>
                                                <Assign.Value>
                                                  <InArgument x:TypeArguments="x:String">Below SAPPrice</InArgument>
                                                </Assign.Value>
                                              </Assign>
                                              <FlowStep.Next>
                                                <FlowDecision x:Name="__ReferenceID1" DisplayName="Below SAP Price 10% &amp; Margin &gt; 18%">
