        Dim LiTstr As String = Util.GetLANGLiT_text(Lit.ID.ToString.Trim)
        If LiTstr.ToString.Trim <> "" Then
            Lit.Text = LiTstr
        End If
    End Sub
    
    Protected Sub btnWiki_Click(ByVal sender As Object, ByVal e As System.EventArgs)
        Dim ws As New SSO.MembershipWebservice
        Dim p As SSO.SSOUSER = ws.getProfile(Session("user_id"), "MY")
        If p IsNot Nothing Then
            Response.Redirect(String.Format("http://wiki.advantech.com/apiLoginAdv.php?action=loginAdv&lgname={0}&lgpassword={1}", Session("user_id"), p.login_password))
        End If
    End Sub
    
    Sub SetPerfImg()
        imgMyPerf.Src = "/Includes/MyPerf.ashx?Year=" + dlPerfYear.SelectedValue + "&Currency=" + dlPerCurr.SelectedValue
    End Sub
    
    Protected Sub dlPerCurr_SelectedIndexChanged(ByVal sender As Object, ByVal e As System.EventArgs)
        SetPerfImg()
    End Sub

    Protected Sub dlPerfYear_SelectedIndexChanged(ByVal sender As Object, ByVal e As System.EventArgs)
        SetPerfImg()
    End Sub

    Protected Sub Page_Load(sender As Object, e As System.EventArgs)
        If Session("account_status") IsNot Nothing Then
            If Session("account_status") = "EZ" Then trChangeCompId.Visible = True
        End If
        If Not Page.IsPostBack Then
            SetPerfImg()
            
            'JJ 2014/3/10 home頁只要是下列company ID："EURA004", "EGBR001", "ELVE001", "ELTG002", "EKZI003", "AHKP006", "ERUP002", "EURP001", "EURP011", "EUAJ001", "EURS006"的就隱藏
            If Session("company_id") IsNot Nothing AndAlso Util.NoShowProjectRegistrationUser(Session("company_id")) Then
                trProjectRegist.Visible = False : trProjectRegistlist.Visible = False
            End If
            
        End If
    End Sub
    
    Public Function IsPhone(ByVal value As String) As Boolean
        If Trim(value) <> "" And Not IsNothing(value) Then
            isPhones = True
        Else
            isPhones = False
        End If
        Return isPhones
    End Function
    
    Public Function IsUserID(ByVal value As String) As Boolean
        If Trim(value) <> "" And Not IsNothing(value) Then
            isUserIDs = True
        Else
            isUserIDs = False
        End If
        Return isUserIDs
    End Function
    
    Protected Sub hlUserID_DataBinding(ByVal sender As Object, ByVal e As System.EventArgs)
        CType(sender, HyperLink).NavigateUrl = "mailto:" + CType(sender, HyperLink).NavigateUrl
