<script runat="server">

    Protected Sub Page_Load(ByVal sender As Object, ByVal e As System.EventArgs)
        If Not Page.IsPostBack Then
            hdnTempId.Value = Session("TempId") : hdnUserId.Value = Session("user_id")
            For i As Integer = 1 To 31
                Dim dt As New DataTable
                With dt.Columns
                    .Add("NO") : .Add("IMAGE") : .Add("DESCRIPTION") : .Add("LANGUAGE") : .Add("SIZE") : .Add("DOC_ID") : .Add("ISSUE_DATE")
                End With
                dt.Rows.Add()
                Dim gv As GridView = FindControlRecursive(Page, "GridView" + i.ToString)
                If gv IsNot Nothing Then
                    gv.DataSource = dt : gv.DataBind()
                    If gv.Rows.Count > 0 Then
                        gv.UseAccessibleHeader = True
                        'gv.HeaderRow.TableSection = TableRowSection.TableHeader
                    End If
                End If
                
            Next
        End If
    End Sub
    
    Private Shared Function FindControlRecursive(ByVal rootControl As Control, ByVal controlID As String) As Control
        If rootControl.ID = controlID Then
            Return rootControl
        End If

        For Each controlToSearch As Control In rootControl.Controls
            Dim controlToReturn As Control = FindControlRecursive(controlToSearch, controlID)
            If controlToReturn IsNot Nothing Then
                Return controlToReturn
            End If
        Next
        Return Nothing
    End Function
    
    <Services.WebMethod()> _
    Public Shared Function GetData(ByVal strKeyword As String, ByVal strPDCategory As String, ByVal strSecCategory As String) As String
        strKeyword = Replace(Replace(Trim(strKeyword), "'", "''"), "*", "%")
        Dim PDCat() As String = strPDCategory.Split(";"), SecCat() As String = strSecCategory.Split(";")
        Dim arrPD As New List(Of String), arrSec As New List(Of String)
        For i As Integer = 0 To PDCat.Length - 1
            If PDCat(i).Trim <> "" Then arrPD.Add("'" + PDCat(i) + "'")
        Next
        For i As Integer = 0 To SecCat.Length - 1
            If SecCat(i).Trim <> "" Then arrSec.Add("'" + SecCat(i) + "'")
        Next
        'Util.SendEmail("rudy.wang@advantech.com.tw", "MyAdvantech@advantech.com", "test", strSecCategory, True, "", "")
        Dim arrSecVal As New List(Of String)
        If arrSec.Count > 0 Then
            Dim dtSecProd As DataTable = dbUtil.dbGetDataTable("MY", String.Format("select distinct b.INTERESTED_PRODUCT_CATEGOEY_ID from CMS_SECTOR_ITEM a left join PIS.dbo.MODELCATEGORY_INTERESTEDPRODUCT_MAPPING b on a.PIS_CATEGORY_ID=b.ITEM_ID or a.PIS_CATEGORY_ID=b.INTERESTED_PRODUCT_CATEGOEY_ID where a.CMSID in ({0})", String.Join(",", arrSec.ToArray())))
            For Each row As DataRow In dtSecProd.Rows
                If Not arrPD.Contains("'" + row.Item("INTERESTED_PRODUCT_CATEGOEY_ID") + "'") Then arrPD.Add("'" + row.Item("INTERESTED_PRODUCT_CATEGOEY_ID") + "'")
            Next
            Dim dtSec As DataTable = dbUtil.dbGetDataTable("MY", String.Format("select value from CMS_SECTOR_LOV where ID in ({0})", String.Join(",", arrSec.ToArray())))
            For Each row As DataRow In dtSec.Rows
                arrSecVal.Add("'" + row.Item("value") + "'")
            Next
        End If
        
        
        Dim sb As New StringBuilder
        With sb
            .AppendLine(" select * from ( ")
            'Get Support
            .AppendLine(" select distinct top 1000 a.DOC_ID, REPLACE(REPLACE(UPPER(a.DOC_TYPE),'/','_'),' ','_') as DOC_TYPE, a.DESCRIPTION as SUBJECT, '' as DESCRIPTION, convert(varchar, b.UPDATED_DATE, 111) as ISSUE_DATE, 0 as SIZE, 'English' as LANGUAGE, '' as HYPER_LINK, a.RECORD_IMG, a.SOURCE, a.DOC_SOURCE as SR_TYPE, 'SUPPORT' as RESOURCE_TYPE, '' as FILE_EXT, a.DOC_TYPE as ORI_DOC_TYPE ")
            .AppendLine(" from SUPPORT_DOWNLOAD a left join SIEBEL_SR_DOWNLOAD b on a.DOC_ID=b.SR_ID ")
            .AppendLine(" left join RESOURCE_MODEL_CATEGORY c on a.DOC_ID=c.DOC_ID ")
            .AppendLine(" where ((a.DOC_TYPE='INSTALLATION' and a.DOC_SOURCE='Download') or a.DOC_TYPE in ('ONLINE_TRAINING','FAQ','BIOS','CERTIFICATE','DRIVER','FIRMWARE','MANUAL','SPECIFICATION','UTILITY','SOFTWARE_API','SOFTWARE_API_MANUAL','DATASHEET') ) ")
            If strKeyword <> "" Then .AppendFormat(" and (a.description like N'%{0}%' or a.search_key like N'%{0}%' or a.abstract like N'%{0}%' or c.MATCH_KEY like N'%{0}%') ", strKeyword)
            If arrPD.Count > 0 Then
                .AppendFormat(" and c.DOC_SOURCE='SUPPORT' and (c.MATCH_INTPROD in ({0}) or c.MATCH_PDGROUP in ({0})) ", String.Join(",", arrPD.ToArray()))
            ElseIf arrPD.Count = 0 AndAlso arrSec.Count > 0 Then
                .AppendFormat(" and 1<>1 ")
            End If
            
            'Get PIS
            .AppendLine(" union all ")
            .AppendLine(" SELECT DISTINCT top 1000 a.LITERATURE_ID AS DOC_ID, case when a.LIT_TYPE like 'Certificate%' then 'CERTIFICATE' when a.LIT_TYPE like '%data%sheet%' then 'DATASHEET' when a.LIT_TYPE in ('brochure','catalogue') then 'BROCHURE_CATALOGUE' ")
            .AppendLine(" when a.LIT_TYPE='Product - Roadmap' then 'PRODUCT_ROADMAPS' when a.LIT_TYPE like 'Product - Photo%' then 'PHOTO' when a.LIT_TYPE='Product - Sales Kit' then 'SALES_KIT' when a.LIT_TYPE in ('Presentation (For CP Only)','Event Presentation') then 'PRESENTATION' ")
            .AppendLine(" when a.LIT_TYPE like 'Advertisement%' then 'ADVERTISEMENT' when a.LIT_TYPE like 'Event Poster%' then 'POSTER' when a.LIT_TYPE like 'poster%' then 'ADVERTISEMENT' when a.LIT_TYPE='Logo: Certificate' then 'CERTIFICATE_LOGO' else UPPER(replace(a.LIT_TYPE,' ','_')) end as DOC_TYPE, ")
            .AppendLine(" a.LIT_NAME as SUBJECT, a.LIT_DESC as DESCRIPTION, CONVERT(varchar, a.LAST_UPDATED, 111) as ISSUE_DATE, cast((ISNull(a.FILE_SIZE, 0)/1024) as int) AS SIZE, a.LANG as LANGUAGE, '' as HYPER_LINK, isnull(c.Thumbnail_ID, '') as RECORD_IMG, '' as SOURCE, '' AS SR_TYPE, 'PIS' as RESOURCE_TYPE, UPPER(a.FILE_EXT) as FILE_EXT, UPPER(a.LIT_TYPE) as ORI_DOC_TYPE ")
            .AppendLine(" FROM PIS.dbo.v_LITERATURE a LEFT JOIN SIEBEL_LITERATURE_DETAIL b ON a.LITERATURE_ID = b.LIT_ID LEFT JOIN PIS.dbo.LITERATURE_EXTEND c ON a.LITERATURE_ID = c.LIT_ID ")
            .AppendLine(" left join RESOURCE_MODEL_CATEGORY d on a.LITERATURE_ID=d.DOC_ID ")
            .AppendLine(" where (a.LIT_TYPE like 'Certificate%' or a.LIT_TYPE like '%data%sheet%' or a.LIT_TYPE like 'Product - Photo%' or a.LIT_TYPE like 'Advertisement%' or a.LIT_TYPE like 'Event Poster%' ")
            .AppendLine(" or a.LIT_TYPE like 'poster%' or a.LIT_TYPE in ('Brochure','CATALOGUE','Product - Roadmap','Press Release','Product - Sales Kit','Presentation (For CP Only)','Event Presentation','Banner','DM','Logo: Certificate')) ")
            If strKeyword <> "" Then .AppendFormat(" and (a.LIT_NAME like N'%{0}%' or a.LIT_DESC like N'%{0}%' or b.TXT_CONTENT like N'%{0}%' or d.MATCH_KEY like N'%{0}%') ", strKeyword)
            If arrPD.Count > 0 Then
                .AppendFormat(" and d.DOC_SOURCE='PIS' and (d.MATCH_INTPROD in ({0}) or d.MATCH_PDGROUP in ({0})) ", String.Join(",", arrPD.ToArray()))
            ElseIf arrPD.Count = 0 AndAlso arrSec.Count > 0 Then
                .AppendFormat(" and 1<>1 ")
            End If
            
            'Get eDM
            .AppendLine(" union all ")
            .AppendLine(" select top 2000 a.ROW_ID as DOC_ID, 'EDM' as DOC_TYPE, a.CAMPAIGN_NAME as SUBJECT, a.DESCRIPTION, CONVERT(varchar, a.ACTUAL_SEND_DATE, 111) as ISSUE_DATE, 0 as SIZE, a.LANGUAGE, '' as HYPER_LINK, '' as RECORD_IMG, '' as SOURCE, '' AS SR_TYPE, 'EDM' as RESOURCE_TYPE, '' as FILE_EXT, 'EDM' as ORI_DOC_TYPE ")
            .AppendLine(" from CAMPAIGN_MASTER a ")
            .AppendLine(" left join RESOURCE_MODEL_CATEGORY b on a.ROW_ID=b.DOC_ID ")
            .AppendLine(" where a.ACTUAL_SEND_DATE is not null and a.IS_PUBLIC=1 ")
            If strKeyword <> "" Then .AppendFormat(" and (a.CAMPAIGN_NAME like N'%{0}%' or a.DESCRIPTION like N'%{0}%' or b.MATCH_KEY like N'%{0}%') ", strKeyword)
            If arrPD.Count > 0 Then
                .AppendFormat(" and b.DOC_SOURCE='EDM' and (b.MATCH_INTPROD in ({0}) or b.MATCH_PDGROUP in ({0})) ", String.Join(",", arrPD.ToArray()))
            ElseIf arrPD.Count = 0 AndAlso arrSec.Count > 0 Then
                .AppendFormat(" and 1<>1 ")
            End If
            
            'Get CMS
            .AppendLine(" union all ")
            '.AppendLine(" select top 1000 a.RECORD_ID as DOC_ID, case a.CATEGORY_NAME when 'eCatalog' then 'BROCHURE_CATALOGUE' when 'eDM / eNewsletter' then 'CAMPAIGN' else REPLACE(UPPER(a.CATEGORY_NAME),' ','_') end as DOC_TYPE, a.TITLE as DESCRIPTION, a.RELEASE_DATE as ISSUE_DATE, 0 as SIZE, 'English' as LANGUAGE, a.HYPER_LINK, case a.RECORD_IMG when 'http://wfcache.advantech.com/EZ/CMSUpLoadFiles/' then '' when 'http://employeezone.advantech.com.tw/CMSUploadFiles/' then '' else '<img src=''' + a.RECORD_IMG + ''' width=''80'' />' end as RECORD_IMG ")
            .AppendLine(" select distinct top 1000 a.RECORD_ID as DOC_ID, case a.CATEGORY_NAME when 'eCatalog' then 'BROCHURE_CATALOGUE' when 'eDM / eNewsletter' then 'CAMPAIGN' else REPLACE(UPPER(a.CATEGORY_NAME),' ','_') end as DOC_TYPE, a.TITLE as SUBJECT, a.ABSTRACT as DESCRIPTION, convert(varchar, a.RELEASE_DATE, 111) as ISSUE_DATE, 0 as SIZE, 'English' as LANGUAGE, a.HYPER_LINK, a.RECORD_IMG, '' as SOURCE, '' AS SR_TYPE, 'CMS' as RESOURCE_TYPE, '' as FILE_EXT, UPPER(a.CATEGORY_NAME) as ORI_DOC_TYPE ")
            .AppendLine(" from WWW_RESOURCES a ")
            .AppendLine(" left join RESOURCE_MODEL_CATEGORY b on a.RECORD_ID=b.DOC_ID ")
            .AppendLine(" left join WWW_RESOURCES_BU c on a.RECORD_ID=c.RECORD_ID ")
            .AppendLine(" where IS_INTERNAL_ONLY=0 and a.CATEGORY_NAME in ('eCatalog','Case Study','Video','White Papers','eDM / eNewsletter','Podcast','Webcast') ")
            If strKeyword <> "" Then .AppendFormat(" and (a.TITLE like N'%{0}%' or a.ABSTRACT like N'%{0}%' or b.MATCH_KEY like N'%{0}%') ", strKeyword)
            If arrPD.Count > 0 AndAlso arrSecVal.Count > 0 Then
                .AppendFormat(" and ( (b.DOC_SOURCE='CMS' and (b.MATCH_INTPROD in ({0}) or b.MATCH_PDGROUP in ({0}))) or (c.BU in ({1})) ) ", String.Join(",", arrPD.ToArray()), String.Join(",", arrSecVal.ToArray()))
            ElseIf arrPD.Count > 0 Then
                .AppendFormat(" and b.DOC_SOURCE='CMS' and (b.MATCH_INTPROD in ({0}) or b.MATCH_PDGROUP in ({0})) ", String.Join(",", arrPD.ToArray()))
            ElseIf arrSec.Count > 0 Then
                If arrSecVal.Count > 0 Then
                    .AppendFormat(" and c.BU in ({0}) ", String.Join(",", arrSecVal.ToArray()))
                Else
                    .AppendFormat(" and 1 <> 1 ")
                End If
            End If
            .AppendLine(" ) as t order by t.ISSUE_DATE desc ")
        End With
        'Util.SendEmail("rudy.wang@advantech.com.tw", "MyAdvantech@advantech.com", "SQL", sb.ToString, True, "", "")
        Dim dt As DataTable = dbUtil.dbGetDataTable("MY", sb.ToString)
        Dim ds As New DataSet
        ds.Tables.Add(dt)
        Return ds.GetXml()
    End Function
    
    <Services.WebMethod()> _
    Public Shared Function GetPG() As String
        Dim sb As New StringBuilder
        With sb
            .AppendLine(" select * from ( ")
            .AppendLine(" select CATEGORY_ID, CATEGORY_ID as PARENT_CATEGORY_ID ,CATALOG_DESC as DISPLAY_NAME, '1' as DEPT from CATALOG_SHOW ")
            .AppendLine(" union all ")
            .AppendLine(" select CATEGORY_ID, PARENT_CATEGORY_ID, DISPLAY_NAME, '2' as DEPT from PIS.dbo.CATEGORY ")
            .AppendLine(" where PARENT_CATEGORY_ID in (select CATEGORY_ID as CATEGORY_NAME from CATALOG_SHOW) ")
            .AppendLine(" ) as t order by t.PARENT_CATEGORY_ID, t.DEPT, t.DISPLAY_NAME ")
        End With
        Dim dt As DataTable = dbUtil.dbGetDataTable("MY", sb.ToString)
        Dim ds As New DataSet
        ds.Tables.Add(dt)
        Return ds.GetXml()
    End Function
    
    <Services.WebMethod()> _
    Public Shared Function GetSector() As String
        Dim sb As New StringBuilder
        With sb
            .AppendLine(" select ID, REPLACE(VALUE,'(Sector)','') as VALUE from CMS_SECTOR_LOV order by VALUE ")
        End With
        Dim dt As DataTable = dbUtil.dbGetDataTable("MY", sb.ToString)
        Dim ds As New DataSet
        ds.Tables.Add(dt)
        Return ds.GetXml()
    End Function
    
    <Services.WebMethod()> _
    <Web.Script.Services.ScriptMethod()> _
    Public Shared Function GetNewsContent(ByVal recid As String, ByVal Type As String) As String
        Try
            Return Util.GetCMSContent(recid, Type)
        Catch ex As Exception
            Util.SendEmail("tc.chen@advantech.eu", "ebiz.aeu@advantech.eu", "Error GetWWWNews", "recid:" + recid + "<br/>" + ex.ToString, False, "", "")
        End Try
        Return "Content currently not available"
    End Function
    
    <Services.WebMethod()> _
    Public Shared Function GetModelNo(ByVal prefixText As String, ByVal count As Integer) As String()
        prefixText = Replace(Replace(Trim(prefixText), "'", "''"), "*", "%")
        Dim dt As DataTable = dbUtil.dbGetDataTable("MY", String.Format("select top 10 ITEM_DISPLAYNAME from PIS.dbo.MODELCATEGORY_INTERESTEDPRODUCT_MAPPING where ITEM_TYPE='Model' and ITEM_DISPLAYNAME like '{0}%' order by ITEM_DISPLAYNAME", prefixText))
        If dt.Rows.Count > 0 Then
            Dim str(dt.Rows.Count - 1) As String
            For i As Integer = 0 To dt.Rows.Count - 1
                str(i) = dt.Rows(i).Item(0)
            Next
            Return str
        End If
        Return Nothing
    End Function
</script>

