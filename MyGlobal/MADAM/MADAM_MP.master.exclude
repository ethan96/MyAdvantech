    End Sub

    Private Sub AddGoogleTrackingToSearch()
        Me.txtSearchBox.Attributes.Add("onkeydown", "SubmitKeyClick('Header1_txtSearchBox',event)")
        Me.txtSearchBox.Attributes.Add("onclick", "GoogleTracking(""/"",""search"",""type the content"") ")
    End Sub

    Private Sub goSearch()
        Dim strURL As String

        If Request.RawUrl.ToLower.IndexOf("products/search/default.aspx") > 0 Then
            strURL = "/products/search/Default.aspx?keyword=" & Me.txtSearchBox.Text
        Else
            strURL = "/products/search.aspx?keyword=" & Me.txtSearchBox.Text
        End If
        Response.Redirect(strURL)
    End Sub
