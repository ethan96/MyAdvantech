  <wa:ACNFindApproverFlow.Implementation>
    <Flowchart DisplayName="Find Approvers">
      <Flowchart.Variables>
        <Variable x:TypeArguments="x:Double" Default="0" Name="Configuration_Margin" />
        <Variable x:TypeArguments="x:Double" Default="0" Name="ApproveLevel" />
        <Variable x:TypeArguments="x:Double" Default="0" Name="MaxApproveLevel" />
        <Variable x:TypeArguments="x:String" Name="Approver" />
        <Variable x:TypeArguments="x:Boolean" Name="IsNeedNormalApprover" />
        <Variable x:TypeArguments="x:Boolean" Default="False" Name="ApproverExisted" />
        <Variable x:TypeArguments="x:Double" Name="Configuration_BelowSAPPriceRate" />
        <Variable x:TypeArguments="x:String" Name="BU_Sector" />
        <Variable x:TypeArguments="scg:List(x:String)" Name="ApproverList" />
      </Flowchart.Variables>
      <Flowchart.StartNode>
        <FlowStep x:Name="__ReferenceID221">
          <Assign DisplayName="Init Approval List">
            <Assign.To>
              <OutArgument x:TypeArguments="scg:List(amd:WorkFlowApproval)">
                <mca:CSharpReference x:TypeArguments="scg:List(amd:WorkFlowApproval)">ApprovalList</mca:CSharpReference>
              </OutArgument>
            </Assign.To>
            <Assign.Value>
              <InArgument x:TypeArguments="scg:List(amd:WorkFlowApproval)">
                <mca:CSharpValue x:TypeArguments="scg:List(amd:WorkFlowApproval)">new List&lt;WorkFlowApproval&gt;()</mca:CSharpValue>
              </InArgument>
            </Assign.Value>
          </Assign>
          <FlowStep.Next>
            <FlowStep x:Name="__ReferenceID109">
              <Assign DisplayName="Get Sales Sector">
                <Assign.To>
                  <OutArgument x:TypeArguments="x:String">
                    <mca:CSharpReference x:TypeArguments="x:String">BU_Sector</mca:CSharpReference>
                  </OutArgument>
                </Assign.To>
                <Assign.Value>
                  <InArgument x:TypeArguments="x:String">
                    <mca:CSharpValue x:TypeArguments="x:String">UserRoleBusinessLogic.getSectorBySalesEmailSalesCode(SalesEmail,SalesCode)</mca:CSharpValue>
                  </InArgument>
                </Assign.Value>
              </Assign>
              <FlowStep.Next>
                <FlowSwitch x:TypeArguments="x:String" x:Name="__ReferenceID110">
                  <FlowSwitch.Default>
                    <FlowStep x:Name="__ReferenceID234">
                      <Assign>
                        <Assign.To>
                          <OutArgument x:TypeArguments="local:FindApproverResult">
                            <mca:CSharpReference x:TypeArguments="local:FindApproverResult">FindApproverResult</mca:CSharpReference>
                          </OutArgument>
                        </Assign.To>
                        <Assign.Value>
                          <InArgument x:TypeArguments="local:FindApproverResult">
                            <mca:CSharpValue x:TypeArguments="local:FindApproverResult">FindApproverResult.SalesSectorNotFound</mca:CSharpValue>
                          </InArgument>
                        </Assign.Value>
                      </Assign>
                    </FlowStep>
                  </FlowSwitch.Default>
                  <FlowSwitch.Expression>
                    <mca:CSharpValue x:TypeArguments="x:String">BU_Sector</mca:CSharpValue>
                  </FlowSwitch.Expression>
                  <FlowDecision x:Key="ISG" x:Name="__ReferenceID222" DisplayName="Is LoosItem Quote?">
                    <FlowDecision.Condition>
                      <mca:CSharpValue x:TypeArguments="x:Boolean">QuoteBusinessLogic.IsACNLoosItemQuote(QuotationDetails)</mca:CSharpValue>
                    </FlowDecision.Condition>
                    <FlowDecision.True>
                      <FlowStep x:Name="__ReferenceID231">
                        <Flowchart DisplayName="Create Approvers by SAP Price &amp;Margin">
                          <Flowchart.Variables>
                            <Variable x:TypeArguments="x:Boolean" Default="False" Name="IsNeedPSMApprover" />
                            <Variable x:TypeArguments="x:Int32" Name="TempMaxApprovalLevel" />
                          </Flowchart.Variables>
                          <Flowchart.StartNode>
                            <FlowStep x:Name="__ReferenceID19">
                              <ForEach x:TypeArguments="amd:QuotationDetail" DisplayName="Create PSM Approvers by SAP Price &amp;Margin">
