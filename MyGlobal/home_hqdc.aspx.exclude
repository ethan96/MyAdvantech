    <Services.WebMethod()> _
    <Web.Script.Services.ScriptMethod()> _
    Public Shared Function MyRecentOrderItems() As String
        Try
            Dim uid As String = "", cid As String = ""
            If HttpContext.Current.Session IsNot Nothing AndAlso HttpContext.Current.Session("user_id") IsNot Nothing _
            AndAlso HttpContext.Current.Session("user_id") <> "" Then
                uid = HttpContext.Current.Session("user_id")
            End If
            Dim CanOrder As Boolean = True
            If HttpContext.Current.Session Is Nothing OrElse HttpContext.Current.Session("company_id") Is Nothing _
            OrElse HttpContext.Current.Session("company_id") = "EDDEAA01" OrElse _
             AuthUtil.IsCanPlaceOrder(uid) = False Then
                CanOrder = False
                HttpContext.Current.Session("COMPANY_CURRENCY_SIGN") = "&euro;"
            End If
            Dim curcy_sign = HttpContext.Current.Session("COMPANY_CURRENCY_SIGN")
            cid = HttpContext.Current.Session("company_id")
            Dim dts(2) As DataTable
            Dim nsb As New System.Text.StringBuilder
            With nsb
                .AppendFormat(" select top 6 a.part_no, a.tumbnail_image_id, a.model_no, a.product_desc, a.seq_no, a.end_date from hqdc_promotion_product a ")
                .AppendFormat(" where '{0}' between a.start_date and a.end_date  ", Now.ToString("yyyy/MM/dd"))
                .AppendFormat(" union ")
                .AppendFormat(" select t.part_no, t.tumbnail_image_id, t.model_no, t.product_desc, '9999' as seq_no, '9999/12/31' as end_date from ")
                .AppendLine(String.Format(" (select top 9 b.part_no, b.tumbnail_image_id, b.model_no, b.product_desc "))
                .AppendLine(String.Format(" from sap_product a inner join PRODUCT_FULLTEXT_NEW b on a.part_no=b.part_no "))
                .AppendLine(String.Format(" where b.tumbnail_image_id is not null and b.product_group not in ('Advantech Global Services','Others') and b.material_group in ('PRODUCT') ")) '
                .AppendLine(String.Format(" order by a.create_date desc) as t "))
                .AppendFormat(" order by seq_no,end_date")
            End With
            Dim n9dt As DataTable = dbUtil.dbGetDataTable("MY", nsb.ToString())
            'If n9dt.Rows.Count = 9 Then
            dts(0) = n9dt.Copy() : dts(1) = n9dt.Copy() : dts(2) = n9dt.Copy()
            dts(0).Rows.Clear() : dts(1).Rows.Clear() : dts(2).Rows.Clear()
            dts(0).ImportRow(n9dt.Rows(0)) : dts(0).ImportRow(n9dt.Rows(1)) : dts(0).ImportRow(n9dt.Rows(2))
            dts(1).ImportRow(n9dt.Rows(3)) : dts(1).ImportRow(n9dt.Rows(4)) : dts(1).ImportRow(n9dt.Rows(5))
            dts(2).ImportRow(n9dt.Rows(6)) : dts(2).ImportRow(n9dt.Rows(7)) : dts(2).ImportRow(n9dt.Rows(8))
            'End If
            'Util.SendEmail("tc.chen@advantech.eu", "ebiz.aeu@advantech.eu", "DTS", "1:" + Util.DataTableToXml(dts(0)) + "<br/>2:" + Util.DataTableToXml(dts(1)) + "<br/>3:" + Util.DataTableToXml(dts(2)), True, "", "")
            'Dim SupportImgUrl As String = ConfigurationManager.AppSettings("SupportImgUrl")
            Dim SupportImgUrl As String = "my.advantech.com"
            Dim sb As New System.Text.StringBuilder
            With sb
                .AppendLine(String.Format("<table width='632'>"))
                Dim dtc As Integer = 0
                For Each dt As DataTable In dts
                    dtc += 1
                    If dtc = 3 Then Exit For
                    .AppendLine(String.Format(" <tr align='center'>"))
                    .AppendLine(String.Format("     <td align='center'>"))
                    .AppendLine(String.Format("         <table width='200' style='height:190px'>"))
                    .AppendLine("                           <tr>")
                    If dt IsNot Nothing Then
                        If dtc = 1 Then
                            Dim i As Integer = 0
                            For Each r As DataRow In dt.Rows
                                If i = 3 Then Exit For
                                .AppendLine("                           <td style='width:200' valign='top'>")
                                .AppendLine("                               <table width='200' height='190'>")
                                .AppendLine(String.Format("                     <tr><td valign='top' align='center' style='font-weight:bold;'><a href='/Product/Model_Detail.aspx?model_no={0}'>{1}</a></td></tr>", r.Item("model_no"), r.Item("part_no")))
                                .AppendLine(String.Format("                     <tr><td valign='top' align='center'>{0}</td></tr>", r.Item("product_desc")))
                                .AppendLine(String.Format("                     <tr><td valign='middle' align='center'><a href='/Product/Model_Detail.aspx?model_no={2}'><img style='height:200px;width:200px;border-width:0px;' src='http://" + SupportImgUrl + "/download/downloadlit.aspx?lit_id={0}' alt='{1}'/></a></td></tr>", r.Item("tumbnail_image_id"), r.Item("part_no"), r.Item("model_no")))
                                If CanOrder Then .AppendLine(String.Format("                     <tr><td valign='bottom' align='center'><a href='/order/cart_add2cartline.aspx?part_no={0}'><img src='/Images/add2cart_yellow_en.gif' border='0px'/></a></td></tr>", r.Item("part_no")))
                                .AppendLine("                               </table>")
                                .AppendLine("                           </td>")
                                
                                i += 1
                            Next
                            
                        Else
                            For Each r As DataRow In dt.Rows
                                .AppendLine("                           <td style='width:200'>")
                                .AppendLine("                               <table width='200'>")
                                .AppendLine(String.Format("                     <tr><td align='center' style='font-weight:bold;'><a href='/Product/Model_Detail.aspx?model_no={0}'>{1}</a></td></tr>", r.Item("model_no"), r.Item("part_no")))
                                .AppendLine(String.Format("                     <tr><td align='center'>{0}</td></tr>", r.Item("product_desc")))
                                .AppendLine(String.Format("                     <tr><td align='center'><a href='/Product/Model_Detail.aspx?model_no={2}'><img style='height:200px;width:200px;border-width:0px;' src='http://" + SupportImgUrl + "/download/downloadlit.aspx?lit_id={0}' alt='{1}'/></a></td></tr>", r.Item("tumbnail_image_id"), r.Item("part_no"), r.Item("model_no")))
                                '.AppendLine(String.Format("                     <tr><th style='color:Navy' align='center'>{0}{1}</th></tr>", curcy_sign, Util.GetSAPPrice(r.Item("part_no"), cid)))
                                If CanOrder Then .AppendLine(String.Format("                     <tr><td valign='top' align='center'><a href='/order/cart_add2cartline.aspx?part_no={0}'><img src='/Images/add2cart_yellow_en.gif' border='0px'/></a></td></tr>", r.Item("part_no")))
                                .AppendLine("                               </table>")
                                .AppendLine("                           </td>")
                            Next
                        End If
                        
                    End If
                    .AppendLine("                           </tr>")
                    .AppendLine(String.Format("         </table>"))
                    .AppendLine(String.Format("     </td>"))
                    .AppendLine(String.Format(" </tr>"))
                Next
                .AppendLine(String.Format("</table>"))
            End With
            Return sb.ToString()
        Catch ex As Exception
            Util.SendEmail("tc.chen@advantech.eu,rudy.wang@advantech.com.tw", "ebiz.aeu@advantech.eu", "Error Myrecent items by " + HttpContext.Current.Session("user_id"), ex.ToString, False, "", "")
        End Try
        Return ""
    End Function

    Protected Sub Page_Init(sender As Object, e As System.EventArgs)
        If Session("account_status") IsNot Nothing Then
            If Session("account_status") = "CP" And Session("RBU") = "HQDC" Then
                
            Else
                If Session("account_status") = "CP" Then Response.Redirect("home_cp.aspx")
                If Session("account_status") = "GA" Then Response.Redirect("home_ga.aspx")
                If Session("account_status") = "KA" Then Response.Redirect("home_ka.aspx")
            End If
        End If
    End Sub
    
</script>

