<script type="text/javascript" src="../EC/Includes/jquery.min.js"></script>
<script type="text/javascript" src="../EC/Includes/jquery.pagination.js"></script>
<script type="text/javascript" src="../EC/Includes/jquery.tablesorter.min.js"></script>
<script type="text/javascript" src="../EC/Includes/jquery.metadata.js"></script>
<script src="../EC/Includes/jquery.cookie.js" type="text/javascript"></script>
<script src="../EC/Includes/jquery.treeview.js" type="text/javascript"></script>
<script type="text/javascript">
    var killScroll = false; 
    var currentTab = 0; var xmlDoc; var tempid; var userid; var tabName;
    var tabClick = new Array(31);
    var gvRowsBind = new Array(31);
    var gvRowsCount = new Array(31);
    $(document).ready(function () {
        tempid = $("#<%=hdnTempId.ClientID%>").val();
        userid = $("#<%=hdnUserId.ClientID%>").val();
        //Default Action
        $("ul.tabs li:first").addClass("active").show(); //Activate first tab
        $(".tab_content:first").show(); //Show first tab content

        tabClick = [].repeat(0, 32);
        gvRowsBind = [].repeat(20, 32);
        //On Click Event
        $("ul.tabs li").click(function () {
            $("ul.tabs li").removeClass("active"); //Remove any "active" class
            $(this).addClass("active"); //Add "active" class to selected tab
            $(".tab_content").hide(); //Hide all tab content
            if ($(this).find('span').text().substring($(this).find('span').text().lastIndexOf("(") + 1, $(this).find('span').text().lastIndexOf(")")) > 0) {
                var activeTab = $(this).find("a").attr("href"); //Find the rel attribute value to identify the active tab + content
                $(activeTab).fadeIn(); //Fade in the active content
                currentTab = this.id.replace("li", "");
                gvIndex = currentTab;
            }
            if (tabClick[gvIndex] == 0) { //Bind data
                tabName = $(".div_gv" + gvIndex).attr("id").replace("div_", "");
                $("[id*=GridView" + gvIndex + "]").first().find("tr:gt(1)").detach(); //Initial Gridview
                BindGridView(gvIndex);
                //Pagination
                //initPagination();
                //pageselectCallback(0);
                tabClick[gvIndex] = 1;
            }

            return false;
        });
        $(".tab_content").hide();
        //        var strKey;
        //        $("#Button1").click(function () {
        //            //var strKey = $("#txtKeyword").text();
        //            alert(strKey);
        //            
        //        });

        var PGchidIdx = 0;
        $.ajax({
            type: "POST",
            url: "MaterialSearch_New.aspx/GetPG",
            data: "{}",
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (data) {
                var xmlPGDoc = $.parseXML(data.d);
                var xmlPG = $(xmlPGDoc);
                var PGData = xmlPG.find("Table1");
                //                $("#PGItems").append("<table cellpadding='3'>");
                //                $.each(PGData, function () {
                //                    $("#PGItems").append('<tr><td style="font-size:small"><input type="checkbox" name="pg_active_col" onclick="GetChecked(this,\'PG\')" id="' + $(this).find("CATEGORY_ID").text() + '" value="' + $(this).find("CATEGORY_NAME").text() + '"> ' + $(this).find("CATEGORY_NAME").text() + '</input></td></tr>');
                //                });
                //                $("#PGItems").append("</table>");

                //----Add Product Group Tree 
                var PGitemsHtml = "";
                $.each(PGData, function (PGIndex) {
                    if ($(this).find("DEPT").text() == "1") {
                        if (PGchidIdx != 0) {
                            PGitemsHtml += '</ul>';
                            PGchidIdx = 0;
                        }
                        if (PGIndex == 0) { PGitemsHtml += '<li>'; }
                        else { PGitemsHtml += '</li><li>'; }
                        PGitemsHtml += '<input type="checkbox" name="pg_active_col" onclick="GetChecked(this,\'PG\')" id="' + $(this).find("CATEGORY_ID").text() + '" value="' + $(this).find("DISPLAY_NAME").text() + '"> ' + $(this).find("DISPLAY_NAME").text() + '</input>';
                    }
                    else {
                        if (PGchidIdx == 0) { PGitemsHtml += '<ul>'; }
                        PGitemsHtml += '<li><input type="checkbox" name="pg_active_col_' + $(this).find("PARENT_CATEGORY_ID").text() + '" onclick="GetChecked(this,\'PG\')" id="' + $(this).find("CATEGORY_ID").text() + '" value="' + $(this).find("DISPLAY_NAME").text() + '"> ' + $(this).find("DISPLAY_NAME").text() + '</input></li>';
                        PGchidIdx += 1;
                    }

                });
                $("#PGItems").append(PGitemsHtml);
            },
            error: function (response) {
                //alert("error");
            }
        });

        $.ajax({
            type: "POST",
            url: "MaterialSearch_New.aspx/GetSector",
            data: "{}",
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (data) {
                var xmlSecDoc = $.parseXML(data.d);
                var xmlSec = $(xmlSecDoc);
                var SecData = xmlSec.find("Table1");
                $("#SecItems").append("<table cellpadding='3'>");
                var colCount = 0;
                $.each(SecData, function () {
                    if (colCount == 0) { $("#SecItems").append("<tr>"); }
                    $("#SecItems").append(' <td style="font-size:small"><input type="checkbox" name="sec_active_col" onclick="GetChecked(this,\'Sec\')" id="' + $(this).find("ID").text() + '" value="' + $(this).find("VALUE").text() + '"> ' + $(this).find("VALUE").text() + '</input></td><td width="10"></td>');
                    colCount += 1;
                    if (colCount == 2) {
                        $("#SecItems").append("</tr>");
                        colCount = 0;
                    }
                });
                if (colCount != 2) { $("#SecItems").append("</tr>"); }
                $("#SecItems").append("</table>");
            },
            error: function (response) {
                //alert("error");
            }
        });

    });

    //Infinite scroll
    $(window).scroll(function () { 
        if ($(window).scrollTop() + 100 >= ($(document).height() - ($(window).height()))) { 
            if (killScroll == false) { //  Keeps the loader from fetching more than once.
                //killScroll = true; // Set killScroll to true, to make sure we do not trigger this code again before it's done running.
                if (gvRowsBind[currentTab] < gvRowsCount[currentTab]) {
                    gvRowsBind[currentTab] += 20;
                    BindGridView(currentTab);
                }
            }
        }
    });

    function BindGridView(gvIndex) {
        var RowBindNum = 0;
        var xml = $(xmlDoc);
        var AllData = xml.find("Table1");
        $("[id*=GridView" + gvIndex + "]").first().find("tr:eq(1)").show(); //Show first blank row
        $.each(AllData, function () {
            var doc_type = $(this).find("DOC_TYPE").text();
            if (tabName == doc_type) {
                if (RowBindNum < gvRowsBind[gvIndex] - 20) { RowBindNum++; }
                else if (RowBindNum >= gvRowsBind[gvIndex] - 20 && RowBindNum < gvRowsBind[gvIndex]) {
                    var row = $("[id*=GridView" + gvIndex + "]").first().find("tr:last-child").clone(true);
                    var columns = new Array(3);
                    switch (doc_type) {
                        case "ONLINE_TRAINING": case "FAQ":
                            columns = new Array(3);
                            columns[0] = "NO"; columns[1] = "SUBJECT"; columns[2] = "DOC_ID"; columns[3] = "ISSUE_DATE";
                            break;
                        case "BIOS": case "CERTIFICATE": case "DRIVER": case "FIRMWARE": case "MANUAL": case "SPECIFICATION": case "UTILITY": case "INSTALLATION":
                        case "SOFTWARE_API": case "SOFTWARE_API_MANUAL": case "PODCAST": case "WEBCAST": case "PRODUCT_ROADMAPS": case "PRESS_RELEASE":
                        case "SALES_KIT": case "PRESENTATION": case "CERTIFICATE_LOGO":
                            columns = new Array(4);
                            columns[0] = "NO"; columns[1] = "SUBJECT"; columns[2] = "SIZE"; columns[3] = "DOC_ID"; columns[4] = "ISSUE_DATE";
                            break;
                        case "DATASHEET": case "WHITE_PAPERS":
                            columns = new Array(5);
                            columns[0] = "NO"; columns[1] = "SUBJECT"; columns[2] = "LANGUAGE"; columns[3] = "SIZE"; columns[4] = "DOC_ID"; columns[5] = "ISSUE_DATE";
                            break;
                        case "BROCHURE_CATALOGUE":
                            columns = new Array(6);
                            columns[0] = "NO"; columns[1] = "RECORD_IMG"; columns[2] = "SUBJECT"; columns[3] = "LANGUAGE"; columns[4] = "SIZE"; columns[5] = "DOC_ID"; columns[6] = "ISSUE_DATE";
                            break;
                        case "CASE_STUDY":
                            columns = new Array(4);
                            columns[0] = "NO"; columns[1] = "SUBJECT"; columns[2] = "LANGUAGE"; columns[3] = "DOC_ID"; columns[4] = "ISSUE_DATE";
                            break;
                        case "VIDEO":
                            columns = new Array(4);
                            columns[0] = "NO"; columns[1] = "RECORD_IMG"; columns[2] = "SUBJECT"; columns[3] = "DOC_ID"; columns[4] = "ISSUE_DATE";
                            break;
                        case "CAMPAIGN": case "EDM": case "PHOTO": case "ADVERTISEMENT": case "BANNER": case "DM": case "POSTER":
                            columns = new Array(5);
                            columns[0] = "NO"; columns[1] = "RECORD_IMG"; columns[2] = "SUBJECT"; columns[3] = "SIZE"; columns[4] = "DOC_ID"; columns[5] = "ISSUE_DATE";
                            break;
                        default:

                    }
                    AppendRow($(this), row, columns); $("[id*=GridView" + gvIndex + "]").first().append(row);
                    RowBindNum++;
                }
                else { return false; }
            }
        });
        $("[id*=GridView" + gvIndex + "]").first().find("tr:eq(1)").hide(); // Hide first blank row
        //Bind Data Index
        BindRowIndex(gvIndex);
    }

    function GetChecked(box, Cat) {
        switch (Cat) {
            case "PG":
                if ($("#" + box.id).attr("checked") == "checked") {
                    $("#pg_choiceitem").append('<div style="display:inline;padding-right:5px;" id="p_' + box.id + '">' + box.value + ' <img src="../Images/close.jpg" id="p_' + box.id + '" onclick="RemoveItem(this,\'PG\')" style="cursor:pointer;" /></div>');
                    if (box.name == "pg_active_col") {
                        $("input[name='pg_active_col_" + box.id + "']").each(function () {
                            $(this).attr("disabled", true);
                            $(this).attr("checked", false);
                            $("div[id=p_" + this.id + "]").each(function () {
                                $(this).remove();
                            });
                            $("#div_pg_id").html($("#div_pg_id").html().replace(this.id + ";", ""));
                        });
                    }
                }
                else {
                    $("div[id=p_" + box.id + "]:last").remove();
                    if (box.name == "pg_active_col") {
                        $("input[name='pg_active_col_" + box.id + "']").each(function () {
                            $(this).removeAttr("disabled");
                        });
                    }
                }
                if ($("#pg_choiceitem").html() != "") {
                    $("#pg_clearItem").show()
                }
                else { $("#pg_clearItem").hide(); }
                break;
            case "Sec":
                if ($("#" + box.id).attr("checked") == "checked") {
                    $("#sec_choiceitem").append('<div style="display:inline;padding-right:5px;" id="s_' + box.id + '">' + box.value + ' <img src="../Images/close.jpg" id="s_' + box.id + '" onclick="RemoveItem(this,\'Sec\')" style="cursor:pointer" /></div>');
                }
                else {
                    $("div[id=s_" + box.id + "]:last").remove();
                }
                if ($("#sec_choiceitem").html() != "") {
                    $("#sec_clearItem").show()
                }
                else { $("#sec_clearItem").hide(); }
                break;
        }
        
    }

    function RemoveItem(rbox, Cat) {
        switch (Cat) {
            case "PG":
                if ($("#" + rbox.id.replace("p_", "")).attr('name') == "pg_active_col") {
                    $("input[name='pg_active_col_" + rbox.id.replace("p_", "") + "']").each(function () {
                        $(this).removeAttr("disabled");
                    });
                }
                $("div[id=" + rbox.id + "]").each(function () {
                    $(this).remove();
                });
                $("#" + rbox.id.replace("p_", "")).attr("checked", false);
                if ($("#pg_choiceitem").html() == "") { $("#pg_clearItem").hide(); }
                $("#div_pg_id").html($("#div_pg_id").html().replace(rbox.id.replace("p_", "") + ";", ""));
                break;
            case "Sec":
                $("div[id=" + rbox.id + "]").each(function () {
                    $(this).remove();
                });
                $("#" + rbox.id.replace("s_", "")).attr("checked", false);
                if ($("#sec_choiceitem").html() == "") { $("#sec_clearItem").hide(); }
                $("#div_sec_id").html($("#div_sec_id").html().replace(rbox.id.replace("s_", "") + ";", ""));
                break;
        }
    }

    Array.prototype.repeat = function (what, L) {
        while (L) this[--L] = what;
        return this;
    }

    var gvIndex;
    function OnSuccess(response) {
        $("#div_err").hide(); 
        currentTab = 0;
        xmlDoc = $.parseXML(response.d);
        var xml = $(xmlDoc);
        var AllData = xml.find("Table1");

        $(".SortTable").each(function (stIndex) {
            var gv = "GridView" + (stIndex + 1);
            //$("[id*=" + gv + "] tr").not($("[id*=" + gv + "] tr:first-child")).remove();
            //$("[id*=" + gv + "]:first tr:gt(1)").detach(); //Initial Gridview
            $("[id*=" + gv + "]").first().find("tr:gt(1)").detach(); //Initial Gridview
            $("[id*=" + gv + "]").first().find("tr:eq(1)").show(); //Show first blank row
            $Itable = $(this);
            $('th', $Itable).each(function (Icolumn) {
                var sColumnTitile = $(this).html();
                sColumnTitile = sColumnTitile.replace('↓', '').replace('↑', '');
                $(this).html(sColumnTitile);
            })
        });

        //Bind data
        gvRowsCount = [].repeat(0, 32);
        $.each(AllData, function () {
            var doc_type = $(this).find("DOC_TYPE").text();
            var gv_id = $("#div_" + doc_type).attr("class");
            if (gv_id != undefined) {
                gv_id = gv_id.replace("div_gv", "");
                gvRowsCount[gv_id] += 1;
            }

        });

        $('table.SortTable').each(function (tIndex) {
            if ($("#num" + (tIndex + 1)).text().lastIndexOf("(") >= 0) { //Calculate Rows count
                var index = $("#num" + (tIndex + 1)).text().lastIndexOf("(") - 1;
                $("#num" + (tIndex + 1)).text($("#num" + (tIndex + 1)).text().substring(0, index));
            }
            $("#num" + (tIndex + 1)).text($("#num" + (tIndex + 1)).text() + " (" + gvRowsCount[tIndex + 1] + ")");

            //Sorting
            var $table = $(this);
            $('th', $table).each(function (column) {
                if ($(this).is('.SortColumn')) {
                    $(this).click(function () {
                        var sColumnTitile = $(this).html();

                        var bSort = (sColumnTitile.indexOf('↑') > -1);
                        sColumnTitile = sColumnTitile.replace('↓', '').replace('↑', '');
                        if (bSort) {
                            $(this).html(sColumnTitile + '&darr;');
                        }
                        else {
                            $(this).html(sColumnTitile + '&uarr;');
                        }
                        //var rows = $table.find('tbody > tr').get();
                        var rows = $("[id*=GridView" + (tIndex + 1) + "]").first().find("tr:gt(1)");

                        rows.sort(function (a, b) {
                            var keyA = $(a).children('td').eq(column).text().toUpperCase();
                            var keyB = $(b).children('td').eq(column).text().toUpperCase();
                            if (bSort) {
                                if (keyA > keyB) return 1;
                                if (keyA < keyB) return -1;
                            }
                            else {
                                if (keyA < keyB) return 1;
                                if (keyA > keyB) return -1;
                            }
                            return 0;
                        });
                        $.each(rows, function (index, row) {
                            $("[id*=GridView" + (tIndex + 1) + "]").first().children('tbody').append(row);
                        })

                        //initPagination();
                        //pageselectCallback(0);
                        BindRowIndex(tIndex + 1);

                        //Clear other columns sorting tag
                        $('th', $table).each(function (column1) {
                            if (column1 != column) {
                                var sColumnTitile = $(this).html();
                                sColumnTitile = sColumnTitile.replace('↓', '').replace('↑', '');
                                $(this).html(sColumnTitile);
                            }
                        })
                    });
                };
            });
        });

        gvIndex = 0;

        $(".tab_content").hide();
        $("ul.tabs li").removeClass("active"); //Remove any "active" class


        $(".SortTable").each(function (stIndex2) {
            if (gvRowsCount[stIndex2] > 0) {
                $("ul.tabs li a[href=\#tab" + (stIndex2) + "]").click(); // Active the first one tab which has data
                currentTab = stIndex2;
                return false;
            }
        });
        $("#img_load").hide();
    }

    function AppendRow(data, row, Columns) {
        for (gvCol = 1; gvCol <= Columns.length - 1; gvCol++) {
            switch (data.find("RESOURCE_TYPE").text()) {
                case "CMS":
                    if (Columns[gvCol] == "RECORD_IMG") {
                        if (data.find(Columns[gvCol]).text() != "" && data.find(Columns[gvCol]).text() != "http://wfcache.advantech.com/EZ/CMSUpLoadFiles/" && data.find(Columns[gvCol]).text() != "http://employeezone.advantech.com.tw/CMSUploadFiles/") {
                            $("td", row).eq(gvCol).html("<img src='" + data.find(Columns[gvCol]).text() + "' width='80px' />");
                        }
                    }
                    else if (Columns[gvCol] == "SUBJECT") {
                        var url = "http://resources.advantech.com.tw/sso/autologin.aspx?tempid=" + tempid + "&id=" + userid + "&pass=MY&callbackurl=http://resources.advantech.com/Resources/Details.aspx?rid=" + data.find("DOC_ID").text()
                        if (data.find("DOC_TYPE").text() == "CASE_STUDY") {
                            $("td", row).eq(gvCol).html('<a href="' + url + '" target="_blank">' + data.find(Columns[gvCol]).text() + '</a><br/><br/>' + data.find("DESCRIPTION").text() + '<br/>' + '<div id="' + data.find("DOC_ID").text() + '"><a href="javascript:void(0);" onclick="GetNews(\'' + data.find("DOC_ID").text() + '\', \'' + data.find("DOC_ID").text() + '\', \'case study\');">Read case study</a><div>');
                        }
                        else {
                            $("td", row).eq(gvCol).html("<a href='" + url + "' target='_blank'>" + data.find(Columns[gvCol]).text() + "</a><br/><br/>" + data.find("DESCRIPTION").text());
                        }
                    }
                    else if (Columns[gvCol] == "DOC_ID") {
                        var url = "http://resources.advantech.com.tw/sso/autologin.aspx?tempid=" + tempid + "&id=" + userid + "&pass=MY&callbackurl=http://resources.advantech.com/Resources/Details.aspx?rid=" + data.find(Columns[gvCol]).text()
                        if (data.find("DOC_TYPE").text() == "WHITE_PAPER") { url = data.find("HYPER_LINK").text(); }
                        $("td", row).eq(gvCol).html("<a href='" + url + "' target='_blank'>Click</a>");
                    }
                    else {
                        $("td", row).eq(gvCol).html(data.find(Columns[gvCol]).text());
                    }
                    break;
                case "SUPPORT":
                    if (Columns[gvCol] == "DOC_ID") {
                        if (data.find("SR_TYPE").text() == "Knowledge Base") {
                            $("td", row).eq(gvCol).html("<a href='../Product/SR_Detail.aspx?SR_ID=" + data.find(Columns[gvCol]).text() + "&C=" + data.find("ORI_DOC_TYPE").text() + "' target='_blank'>Download</a>");
                        }
                        else {
                            if (data.find("DOC_TYPE").text() == "MANUAL" && data.find("SR_TYPE").text() == "") {
                                $("td", row).eq(gvCol).html("<a href='http://downloadt.advantech.com/download/downloadlit.aspx?lit_id=" + data.find(Columns[gvCol]).text() + "' target='_blank'>Download</a>");
                            }
                            else if (data.find("DOC_TYPE").text() == "CERTIFICATE" && data.find("SOURCE").text() == "PLM") {
                                $("td", row).eq(gvCol).html("<a href='http://downloadt.advantech.com/productfile/PLM_Cer/" + data.find("RECORD_IMG").text() + "' target='_blank'>Download</a>");
                            }
                            else {
                                $("td", row).eq(gvCol).html("<a href='../Product/SR_Download.aspx?SR_ID=" + data.find(Columns[gvCol]).text() + "&C=" + data.find("ORI_DOC_TYPE").text() + "' target='_blank'>Download</a>");
                            }
                        }

                    }
                    else {
                        $("td", row).eq(gvCol).html(data.find(Columns[gvCol]).text());
                    }
                    break;
                case "EDM":
                    if (Columns[gvCol] == "RECORD_IMG") {
                        $("td", row).eq(gvCol).html("<img src='http://my.advantech.com/Includes/GetThumbnail.ashx?RowId=" + data.find("DOC_ID").text() + "' width='80px' />");
                    }
                    else if (Columns[gvCol] == "SUBJECT") {
                        var url = "http://my.advantech.com/Includes/GetTemplate.ashx?RowId=" + data.find("DOC_ID").text()
                        $("td", row).eq(gvCol).html("<a href='" + url + "' target='_blank'>" + data.find(Columns[gvCol]).text() + "</a><br/><br/>" + data.find("DESCRIPTION").text());
                    }
                    else if (Columns[gvCol] == "DOC_ID") {
                        var url = "http://my.advantech.com/Includes/GetTemplate.ashx?RowId=" + data.find(Columns[gvCol]).text()
                        $("td", row).eq(gvCol).html("<a href='" + url + "' target='_blank'>Click</a>");
                    }
                    else {
                        $("td", row).eq(gvCol).html(data.find(Columns[gvCol]).text());
                    }
                    break;
                case "PIS":
                    if (Columns[gvCol] == "RECORD_IMG") {
                        switch (data.find("FILE_EXT").text()) {
                            case "JPG": case "GIF": case "JPEG": case "PNG": case "BMP":
                                $("td", row).eq(gvCol).html("<img src='http://downloadt.advantech.com/download/downloadlit.aspx?lit_id=" + data.find("DOC_ID").text().toString().split('.')[0] + "' width='80px' />");
                                break;
                            case "TIF": case "TIFF":
                                $("td", row).eq(gvCol).html("<img src='../Includes/TIFF_Handler.ashx?LIT_ID=" + data.find("DOC_ID").text().toString().split('.')[0] + "' width='80px' />");
                                break;
                            case "PDF":
                                if (data.find(Columns[gvCol]).text() != "") { $("td", row).eq(gvCol).html("<img src='http://downloadt.advantech.com/download/downloadlit.aspx?lit_id=" + data.find(Columns[gvCol]).text() + "' width='80px' />"); }
                                break;
                        }
                    }
                    else if (Columns[gvCol] == "DOC_ID") {
                        if (data.find("DOC_TYPE").text() == "PRODUCT_ROADMPAS" || data.find("DOC_TYPE").text() == "SALES_KIT" || data.find("ORI_DOC_TYPE").text() == "PRESENTATION (FOR CP ONLY)") {
                            var url = "../Includes/SpecialMaterialDownload.ashx?LIT_ID=" + data.find(Columns[gvCol]).text();
                            $("td", row).eq(gvCol).html("<a href='" + url + "' target='_blank'>Download</a>");
                        }
                        else {
                            var url = "../Product/Unzip_File.aspx?Literature_Id=" + data.find(Columns[gvCol]).text();
                            $("td", row).eq(gvCol).html("<a href='" + url + "' target='_blank'>" + data.find("FILE_EXT").text() + "</a>");
                        }
                    }
                    else {
                        $("td", row).eq(gvCol).html(data.find(Columns[gvCol]).text());
                    }
                    break;
            }
            if (Columns[gvCol] == "RECORD_IMG") {
                $("td", row).eq(gvCol).css("padding-top", "20px");
                $("td", row).eq(gvCol).css("padding-left", "20px");
            }
            if (Columns[gvCol] == "SIZE") { $("td", row).eq(gvCol).html(formatNumber(data.find(Columns[gvCol]).text()) + "k"); }
        }
    }

    function formatNumber(number) {
        x = number.split('.');
        x1 = x[0];
        x2 = x.length > 1 ? '.' + x[1] : '';
        var rgx = /(\d+)(\d{3})/;
        while (rgx.test(x1)) {
            x1 = x1.replace(rgx, '$1' + ',' + '$2');
        }
        return x1 + x2;
    }

    function BindRowIndex(gvIndex) {
        $("[id*=GridView" + gvIndex + "]").first().find("tr").each(function (rIdx) {
            if (rIdx >= 2) { $("[id*=GridView" + gvIndex + "]").first().find("tr:eq(" + rIdx + ") td:eq(0)").html(rIdx - 1); }
        });
    }

    function GetData() {
        tabClick = [].repeat(0, 32);
        var strKey = $.trim($("#<%=txtKeyword.ClientID%>").val());
        var strPDCat = $("#div_pg_id").html();
        strPDCat = strPDCat.substr(0, strPDCat.length - 1);
        var strSecCat = $("#div_sec_id").html();
        strSecCat = strSecCat.substr(0, strSecCat.length - 1);
        if (strKey.length == 0 && strPDCat == "" && strSecCat == "") { $("#div_err").show(); $("#div_err").html("Please input at least one search criteria in Step 1"); return false; }
        if (strKey.length <= 2 && strPDCat == "" && strSecCat == "") { $("#div_err").show(); $("#div_err").html("Please enter at least 3 characters"); return false; }
        $("#img_load").show();
        strKey = strKey.replace("'", "''");
        var postData = JSON.stringify({ strKeyword: strKey, strPDCategory : strPDCat, strSecCategory : strSecCat });
        $.ajax({
            type: "POST",
            url: "MaterialSearch_New.aspx/GetData",
            data: postData,
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            //                success: function (data) {
            //                    for (var i = 0; i < data.d.length; i++) {
            //                        $("#GridView1").append("<tr><td>" + data.d[i].DOC_ID + "</td><td>" + data.d[i].DOC_TYPE + "</td><td>" + data.d[i].DESCRIPTION + "</td></tr>");
            //                    }
            //                },
            success: OnSuccess,
            error: function (response) {
                //alert("error");
                console.log("error : " + response);
                $("#div_err").show(); $("#div_err").html("System Error"); 
                $("#img_load").hide();
            }
        });
    }

    var ItemsPerPage = 10;
    function pageselectCallback(page_index, jq) {
        if (currentTab != 0) { gvIndex = currentTab; }
        $("[id*=GridView" + gvIndex + "]").first().find("tr").each(function (n) {
            //var index = $("[id*=GridView" + gvIndex + "] tr:eq(" + n + ") td:eq(0)").text();
            if (n >= 2) {
                var PageIndex = Math.floor((n - 2) / 10);

                if (PageIndex == page_index) {
                    $("[id*=GridView" + gvIndex + "]").first().find("tr:eq(" + n + ")").show();
                }
                else {
                    $("[id*=GridView" + gvIndex + "]").first().find("tr:eq(" + n + ")").hide();
                }
            }
        });
        return false;
    }

    function initPagination() {
        var num_entries = $("[id*=GridView" + gvIndex + "]").first().find("tr").length - 2;
        // Create content inside pagination element
        $("#Pagination" + gvIndex).pagination(num_entries, {
            callback: pageselectCallback,
            items_per_page: 10, // Show how many items per page
            num_display_entries: 5, // Number of pagination links shown
            num_edge_entries: 2 // Number of start and end points
        });
    }
    var initialTree = true;
    function OpenList(ListCat) {
        switch (ListCat) {
            case "PG":
                $("#pglist").show();
                if (initialTree) {
                    $("#PGItems").treeview({
                        collapsed: true, //是否預先打開
                        animated: "fast", //動畫使用
                        persist: "cookie",
                        cookieId: "navigationtree",
                        prerendered: false, //預先載入同 Layer 內容
                        unique: true //打開某選項，關閉其他選項
                    });
                    initialTree = false;
                }
                $("#seclist").hide();
                break;
            case "Sec":
                $("#pglist").hide();
                $("#seclist").show();
                break;
        }
    }
    function CloseList(ListCat) {
        switch (ListCat) {
            case "PG":
                $("#pglist").hide();
                break;
            case "Sec":
                $("#seclist").hide();
                break;
        }
    }
    function RemoveItems(ListCat) {
        switch (ListCat) {
            case "PG":
                //$("input[name='pg_active_col']").each(function () {
                $("[name*=pg_active_col]").each(function () {
                    $(this).attr("checked", false);
                    $(this).removeAttr("disabled");
                });
                $("#pg_choiceitem").html('');
                $("#pg_clearItem").hide();
                break;
            case "Sec":
                $("input[name='sec_active_col']").each(function () {
                    $(this).attr("checked", false);
                });
                $("#sec_choiceitem").html('');
                $("#sec_clearItem").hide();
                break;
        }
    }
    function Confirm(ListCat) {
        switch (ListCat) {
            case "PG":
                $("#div_pg").html(''); $("#div_pg_id").html('');
                $("input[name='pg_active_col']").each(function () {
                    if ($(this).attr("checked") == "checked") {
                        $("#div_pg").append('<div style="display:inline;padding-right:5px;" id="p_' + $(this).attr("id") + '">' + $(this).attr("value") + ' <img src="../Images/close.jpg" id="p_' + $(this).attr("id") + '" onclick="RemoveItem(this,\'PG\')" style="cursor:pointer" /></div>');
                        $("#div_pg_id").append($(this).attr("id") + ";");
                    }
                });
                $("#pglist").hide();
                break;
            case "Sec":
                $("#div_sec").html(''); $("#div_sec_id").html('');
                $("input[name='sec_active_col']").each(function () {
                    if ($(this).attr("checked") == "checked") {
                        $("#div_sec").append('<div style="display:inline;padding-right:5px;" id="s_' + $(this).attr("id") + '">' + $(this).attr("value") + ' <img src="../Images/close.jpg" id="s_' + $(this).attr("id") + '" onclick="RemoveItem(this,\'Sec\')" style="cursor:pointer" /></div>');
                        $("#div_sec_id").append($(this).attr("id") + ";");
                    }
                });
                $("#seclist").hide();
                break;
        }
    }
