<script type="text/javascript">
    var timeout = 500;
    var closetimer = 0;
    var ddmenuitem = 0;
    var senderDiv = 0;
    // open hidden layer
    function mopen(id, sender) {
        // cancel close timer
        mcancelclosetime();
        // close old layer
        if (ddmenuitem) {
            ddmenuitem.style.visibility = 'hidden';
            senderDiv.style.backgroundColor = '#ffffff';
        }
        // get new layer and show it
        ddmenuitem = document.getElementById(id);
        if (ddmenuitem) {
        ddmenuitem.style.visibility = 'visible';
        sender.style.backgroundColor = '#f0f0f0';
        senderDiv = sender;}
    }
    // close showed layer
    function mclose() {
        if (ddmenuitem) {
            ddmenuitem.style.visibility = 'hidden';
            senderDiv.style.backgroundColor = '#ffffff';
        }
    }
    // go close timer
    function mclosetime() {
        closetimer = window.setTimeout(mclose, timeout);
    }
    // cancel close timer
    function mcancelclosetime() {
        if (closetimer) {
            window.clearTimeout(closetimer);
            closetimer = null;
        }
    }
    // close layer when click-out
    document.onclick = mclose; 

</script>
