    Protected Sub ImageButton1_Click(sender As Object, e As System.Web.UI.ImageClickEventArgs)
        If Validation() Then
            Dim name As String = Replace(txtName.Text.Trim(), "'", "''")
            Dim company As String = Replace(txtCompany.Text.Trim(), "'", "''")
            Dim phone As String = Replace(txtPhone.Text.Trim(), "'", "''")
            Dim sendto_email As String = Replace(txtEmail.Text.Trim(), "'", "''")

            'current using product
            Dim arrGroup As New ArrayList
            For i As Integer = 1 To 4
                Dim cb As CheckBox = CType(Me.up1.FindControl("CheckBoxCurrUseProd" + i.ToString), CheckBox)
                If cb.Checked Then arrGroup.Add(cb.Text)
            Next
            If txtComment1.Text.Trim.Replace("'", "") <> "" Then arrGroup.Add("其他:" + txtComment1.Text.Trim.Replace("'", "''"))
            Dim _usingproduct As String = String.Join("|", arrGroup.ToArray())
            
            'application domain
            Dim arrGroup1 As New ArrayList
            For i As Integer = 1 To 4
                Dim cb As CheckBox = CType(Me.up1.FindControl("CheckBoxAppDomain" + i.ToString), CheckBox)
                If cb.Checked Then arrGroup1.Add(cb.Text)
            Next
            If txtComment2.Text.Trim.Replace("'", "") <> "" Then arrGroup1.Add("其他:" + txtComment2.Text.Trim.Replace("'", "''"))
            Dim _aplicationdomain As String = String.Join("|", arrGroup1.ToArray())

            Dim _IsBorrow As String = ""
            If Me.RadioButtonBorrow1.SelectedIndex > -1 Then _IsBorrow = Me.RadioButtonBorrow1.SelectedValue
            
            
            Dim sb As New System.Text.StringBuilder
            With sb
                .AppendLine(String.Format(" INSERT INTO CAMPAIGN_SURVEYS "))
                .AppendLine(String.Format(" (CAMP_ID, CONTACT_NAME, CONTACT_COMPANY, CONTACT_TEL, CONTACT_EMAIL,  "))
                .AppendLine(String.Format(" INTERESTED_COURSES, REQ_BROCHURE, SUBMIT_TIME, CLIENT_ADDR, CONTACT_ADDRESS, PRODUCT) "))
                .AppendLine(String.Format(" VALUES ('Data Retrieve Card Campaign', N'" + name + "', N'" + company + "', N'" + phone + "', " + _
                                          " N'" + sendto_email + "', N'研華「動動筆得好禮」活動', '0', GETDATE(), '" + Util.GetClientIP() + "', N'', N'Data Retrieve Card') "))

            End With
            If dbUtil.dbExecuteNoQuery("MYLOCAL", sb.ToString()) = 1 Then
                lbMsg.Text = "謝謝您，我們已收到您的資料"
                Dim body As String = "<html><table>" + _
                    "<tr><td>親愛的研華客戶 您好:</td></tr>" + _
                    "<tr><td height='5'></td></tr>" + _
                    "<tr><td>感謝您參與研華資料擷取卡支援LabVIEW「動動筆得好禮」活動，以下是您所填寫的問卷資料<br>" + _
                    "收到您的資料後，近期內將會有專人和您做進一步的連絡，敬請耐心等候。</td></tr>" + _
                    "<tr><td height='5'></td></tr>" + _
                    "<tr><td height='5'>即日起至2012年9月30日止，研華兩款熱銷資料擷取PCI卡最低 $ 2,799起，歡迎來電洽詢0800-777-111或buy@advantech.tw</td></tr>" + _
                    "<tr><td height='5'></td></tr>" + _
                    "</table>" + _
                    "<table>" + _
                    "<tr><th align='right'>姓名: </th><td width='200'>" + name + "</td></tr>" + _
                    "<tr><th align='right'>公司: </th><td width='200'>" + company + "</td></tr>" + _
                    "<tr><th align='right'>電話: </th><td width='200'>" + phone + "</td></tr>" + _
                    "<tr><th align='right'>Email: </th><td width='200'>" + sendto_email + "</td></tr>" + _
                    "<tr><th align='right'>目前使用的卡片品牌: </th><td width='200'>" + _usingproduct + "</td></tr>" + _
                    "<tr><th align='right'>卡片主要應用領域: </th><td width='200'>" + _aplicationdomain + "</td></tr>" + _
                    "<tr><th align='right'>申請借測: </th><td width='200'>" + _IsBorrow + "</td></tr>" + _
                    "</table>" + _
                    "<table>" + _
                    "<tr><td height='5'></td></tr>" + _
                    "<tr><td>研華(股)公司 工業自動化事業群<br>台灣區行銷部<br>敬上</td></tr>" + _
                    "</table></html>"
                Dim _errmsg As String = String.Empty
                Dim _from As String = "myadvanteh@advantech.com"
                Dim htmlMessage As Net.Mail.MailMessage
                htmlMessage = New Net.Mail.MailMessage(_from, sendto_email, "感謝您參與研華「動動筆得好禮」活動", body)
                htmlMessage.IsBodyHtml = True
                
                'Send email to customer who interested with this event
                'SendFromAmazon is not stable enought
                'MailUtil.SendFromAmazon(htmlMessage, "myadvanteh@advantech.com", sendto_email, Nothing, _errmsg)
                Call MailUtil.Utility_EMailPage(_from, sendto_email, "", "Barbara.Sun@advantech.com.tw;Frank.Chung@advantech.com.tw", "感謝您參與研華「動動筆得好禮」活動", Nothing, body)
                
                'Send email to event owner
                Util.SendEmail("Barbara.Sun@advantech.com.tw", "ebiz.aeu@advantech.eu", sendto_email & "登記了研華「動動筆得好禮」活動", body, True, "", "")
                'Util.SendEmail("Frank.Chung@advantech.com.tw", "ebiz.aeu@advantech.eu", sendto_email & "登記了研華「動動筆得好禮」活動", body, True, "", "")
                
                ResetAll()
            End If
        End If
    End Sub
    
    
    Function Validation() As Boolean
        lbMsg.Text = ""
        If txtName.Text.Trim() = "" Then
            lbMsg.Text = "請輸入姓名" : Return False
        End If
        If txtCompany.Text.Trim() = "" Then
            lbMsg.Text = "請輸入公司" : Return False
        End If
        If txtPhone.Text.Trim() = "" Then
            lbMsg.Text = "請輸入電話" : Return False
        End If
        If txtEmail.Text.Trim() = "" Then
            lbMsg.Text = "請輸入Email" : Return False
        End If

        If Util.IsValidEmailFormat(txtEmail.Text.Trim()) = False Then
            lbMsg.Text = "請輸入正確Email格式" : Return False
        End If
        Dim TickAny1 As Boolean = False
        Dim TickAny2 As Boolean = False
        Dim cb As CheckBox = Nothing
        '目前使用的卡片品牌
        For i As Integer = 1 To 4
            cb = CType(Me.up1.FindControl("CheckBoxCurrUseProd" + i.ToString), CheckBox)
            If cb.Checked Then
                TickAny1 = True
                Exit For
            End If
        Next
        If TickAny1 = False Then
            
            If Me.CheckBoxCurrUseProd5.Checked = False AndAlso txtComment1.Text.Trim.Replace("'", "") = "" Then
                lbMsg.Text = "請選擇至少一項目前使用的卡片品牌" : Return False
            End If

            
            If Me.CheckBoxCurrUseProd5.Checked AndAlso txtComment1.Text.Trim.Replace("'", "") = "" Then
                lbMsg.Text = "請輸入目前使用的卡片品牌-其它選項的描述" : Return False
            End If
        End If
            
        'application domain
        Dim arrGroup1 As New ArrayList
        For i As Integer = 1 To 4
            cb = CType(Me.up1.FindControl("CheckBoxAppDomain" + i.ToString), CheckBox)
            If cb.Checked Then
                TickAny2 = True
                Exit For
            End If
        Next
        If TickAny2 = False Then
            
            If Me.CheckBoxAppDomain5.Checked = False AndAlso txtComment2.Text.Trim.Replace("'", "") = "" Then
                lbMsg.Text = "請選擇至少一項卡片主要應用領域" : Return False
            End If
            
            If Me.CheckBoxAppDomain5.Checked AndAlso txtComment2.Text.Trim.Replace("'", "") = "" Then
                lbMsg.Text = "請輸入卡片主要應用領域-其它選項的描述" : Return False
            End If
        End If


        Return True
    End Function
    
